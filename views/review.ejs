
<div class="title-page-deg">
    <p class="title-pages">Reviews Done</p>
    </div>

<% reviews.forEach(review => { %>
  <div class="container">
  <div class="offers-container">
    Coupon ID: <%= review.couponId %>
  <div>
    Restaurant: <%= review.name %>
  </div>
  <div>
    User Rating: <%= review.rate %>
  </div>
  <div>
    User Comment: <%= review.comment %>
  </div>
</div>
</div>
<%});%>


